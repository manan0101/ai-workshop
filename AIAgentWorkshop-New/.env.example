# # AI Provider Selection (choose 'sambanova' or 'ollama')
AI_PROVIDER=ollama

# # SambaNova API Configuration
# SAMBA_API_KEY=<API KEY>

# # Optional: Specify SambaNova model (default is gpt-oss-120b)
# SAMBA_MODEL=gpt-oss-120b

# # Ollama Configuration (for local models)
OLLAMA_MODEL=gemma3:4b

# # NVIDIA API Configuration (optional)
# NVIDIA_API_KEY=<Your-API-KEY>

# # Workshop Configuration
WORKSHOP_DEBUG=false
MAX_TOKENS=4000
TEMPERATURE=0.7